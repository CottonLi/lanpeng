<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanpeng.data.mapper.TFPEMapper">

    <resultMap type="TFPE" id="TFPEResult">
        <result property="id"         column="id"      />
        <result property="name"       column="name"   />
        <result property="sex"        column="sex" />
        <result property="age"        column="age" />
    </resultMap>

    <insert id="addT" parameterType="TFPE" statementType="CALLABLE">
        {call addt(#{id,mode=OUT,jdbcType=INTEGER},#{name,mode=IN},#{sex,mode=IN},#{age,mode=IN})}
    </insert>

    <update id="updateT" parameterType="TFPE" statementType="CALLABLE">
        {call updatet(#{id,mode=IN},#{name,mode=IN},#{sex,mode=IN},#{age,mode=IN})}
    </update>

    <delete id="deleteT" parameterType="Integer" statementType="CALLABLE">
        {call deletet(#{id,mode=IN})}
    </delete>

    <select id="getbyidT" parameterType="Integer" resultMap="TFPEResult" statementType="CALLABLE">
        {call getbyidt(#{id,mode=IN})}
    </select>

</mapper>